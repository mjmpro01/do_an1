     DATPHONG."NGAYDI" AS DATPHONG_NGAYDI,
     DATPHONG."TRANGTHAI" AS DATPHONG_TRANGTHAI
FROM
     "QLSV"."DATPHONG" DATPHONG INNER JOIN "QLSV"."HOADON" HOADON ON DATPHONG."MADP" = HOADON."MADP"
WHERE
	DATPHONG.MAPHONG = $P{MAPHONG}
